{
  "name": "angular2-full-stack",
  "version": "5.2.0",
  "license": "MIT",
  "author": "Davide Violante",
  "description": "Angular Full Stack project built using Angular 2+, Express, Mongoose and Node.",
  "angular-cli": {},
  "engines": {
    "node": "9.x",
    "npm": "5.8.0"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod\" \"ng serve --proxy-config proxy.conf.json -o\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build -aot -prod && tsc -p server && node dist/server/app.js\"",
    "lint": "npm run lint:style && npm run lint:front && npm run lint:html && npm run lint:back",
    "lint:style": "stylelint 'client/**/**/*{.css,.scss}'",
    "lint:front": "ng lint",
    "lint:back": "tslint --project tsconfig.json server/**/**.ts{,x}",
    "lint:html": "htmllint client/**/**/*.html",
    "lint:autofix": "ng lint --fix && tslint --project tsconfig.json server/**/**.ts{,x} --fix && stylelint 'client/**/*{.css,.scss}' --fix",
    "test:front": "ng test --watch=false",
    "test:back": "tsc -p server && mocha dist/server/test --exit",
    "e2e": "ng e2e",
    "start:server": "nodemon server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.3",
    "@angular/cdk": "^7.1.1",
    "@angular/common": "^7.1.3",
    "@angular/compiler": "^7.1.3",
    "@angular/core": "^7.1.3",
    "@angular/forms": "^7.1.3",
    "@angular/http": "^7.1.3",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "^7.1.3",
    "@angular/platform-browser-dynamic": "^7.1.3",
    "@angular/platform-server": "^7.1.3",
    "@angular/router": "^7.1.3",
    "@auth0/angular-jwt": "~2.1.0",
    "aos": "^2.3.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "core-js": "^2.6.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "gridfs-stream": "^1.1.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.0.13",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "multer-gridfs-storage": "^3.0.1",
    "npm": "^6.5.0",
    "pokemon": "^1.2.3",
    "popper.js": "^1.14.3",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "tether": "1.4.5",
    "tslib": "^1.9.0",
    "yo": "^2.0.5",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "^7.1.3",
    "@angular/compiler-cli": "^7.1.3",
    "@angular/language-service": "^7.1.3",
    "@types/express": "^4.11.0",
    "@types/jasmine": "^3.3.2",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^10.12.15",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "codelyzer": "^4.5.0",
    "concurrently": "^4.1.0",
    "htmllint-cli": "^0.0.7",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "mocha": "^5.0.5",
    "nodemon": "^1.18.9",
    "protractor": "^5.4.1",
    "rxjs-tslint": "^0.1.6",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "~7.0.1",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "3.1.6",
    "webpack": "^4.27.1"
  }
}
